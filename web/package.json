{
  "name": "spydr_web",
  "version": "1.0.0",
  "description": "React/Redux interface to my spydr feeds",
  "main": "index.js",
  "scripts": {
    "start-dev": "webpack-dev-server --allow-file-access-from-files --inline --hot --history-api-fallback --host localhost --port 7070 --open",
    "build-dev": "webpack --config webpack.dev.config.js",
    "build-prod": "webpack -p --config webpack.prod.config.js",
    "lint": "./node_modules/.bin/eslint src/**/*.js --ignore-pattern node_modules/",
    "test": "jest --verbose",
    "test-watch": "jest --verbose --watch",
    "start": "node server.js"
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ],
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "author": "James Rutledge",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.6.2",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "3.2.0",
    "ejs": "^2.6.1",
    "ejs-loader": "^0.5.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "25.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.13.1",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.4.0",
    "redux-logger": "^3.0.6",
    "sass-loader": "^6.0.6",
    "url-loader": "^1.1.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "connect-history-api-fallback": "^1.5.0",
    "crypto-js": "^3.1.9-1",
    "dotenv-webpack": "^1.5.7",
    "express": "^4.16.3",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "identity-obj-proxy",
      "~(.*)$": "<rootDir>/src/$1"
    },
    "testURL": "http://localhost"
  }
}
